---
import Sheet from "@/components/Sheet.astro"
import LibraryLayout from "@/layouts/LibraryLayout.astro"
import CPricingSection from "@/components/PricingSection.astro"
import { pricingSectionContent } from "@/lib/content"
---

<LibraryLayout>
	<div class="flex flex-col justify-center items-center w-full min-h-screen">
		<Sheet>
			<CPricingSection
				eyebrow={pricingSectionContent.eyebrow}
				title={pricingSectionContent.title}
				description={pricingSectionContent.description}
				plans={pricingSectionContent.plans}
			/>
		</Sheet>
	</div>
</LibraryLayout>
