---
import Sheet from "@/components/Sheet.astro"
import LibraryLayout from "@/layouts/LibraryLayout.astro"
import CFaqSection from "@/components/FaqSection.astro"
import { faqSectionContent } from "@/lib/content"
---

<LibraryLayout>
	<div class="flex flex-col justify-center items-center w-full min-h-screen">
		<Sheet>
			<CFaqSection
				title={faqSectionContent.title}
				description={faqSectionContent.description}
				faqs={faqSectionContent.faqs}
				cta={faqSectionContent.cta}
			/>
		</Sheet>
	</div>
</LibraryLayout>
